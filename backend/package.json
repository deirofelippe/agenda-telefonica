      "test": "cross-env NODE_ENV=test npx jest --config jest.config.js --runInBand --detectOpenHandles",
      "posttest": "cross-env NODE_ENV=test npx sequelize db:migrate:undo:all",
      "pretest:w": "cross-env NODE_ENV=test npx sequelize db:migrate",
      "test:w": "cross-env NODE_ENV=test npx jest --config jest.config.js --runInBand --watch",
